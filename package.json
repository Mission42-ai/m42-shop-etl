{
  "name": "m42-shop-etl",
  "version": "1.0.0",
  "description": "ETL pipeline for shop product extraction and vectorization",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "crawl": "tsx src/scripts/startCrawl.ts",
    "webhook": "tsx src/server.ts",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "rag:setup": "tsx scripts/setupRAGIndexes.ts",
    "rag:test": "tsx scripts/testRAG.ts",
    "mcp:server": "tsx src/mcp/server-standalone.ts",
    "mcp:dev": "tsx watch src/mcp/server-standalone.ts",
    "mcp:start": "nohup tsx src/mcp/server-standalone.ts > mcp-server.log 2>&1 &",
    "mcp:pm2": "pm2 start pm2.config.js",
    "analyze": "tsx scripts/analyzeFailures.ts",
    "list": "tsx scripts/listProducts.ts",
    "stats": "tsx scripts/getStats.ts",
    "reprocess": "tsx scripts/reprocessFailedProducts.ts"
  },
  "dependencies": {
    "@mendable/firecrawl-js": "^1.4.3",
    "@supabase/supabase-js": "^2.39.0",
    "bull": "^4.12.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.36.0",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "openai": "^4.56.0",
    "postgres": "^3.4.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "drizzle-kit": "^0.29.0",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
